<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/ChlebText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nie jest to na twojej liście."
        app:layout_constraintBottom_toTopOf="@+id/P2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.455"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/P1"
        app:layout_constraintVertical_bias="0.491" />

    <TextView
        android:id="@+id/HerbataText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Nie jest to na twojej liście."
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.455"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/P4" />

    <TextView
        android:id="@+id/MlekoText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nie jest to na twojej liście."
        app:layout_constraintBottom_toTopOf="@+id/P3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.455"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/P2"
        app:layout_constraintVertical_bias="0.537" />

    <CheckedTextView
        android:id="@+id/P4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Herbata"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.465"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.613" />

    <CheckedTextView
        android:id="@+id/P1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Chleb"
        app:layout_constraintBottom_toTopOf="@+id/P2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.67" />

    <CheckedTextView
        android:id="@+id/P3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Masło"
        app:layout_constraintBottom_toTopOf="@+id/P4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.462"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.834" />

    <TextView
        android:id="@+id/MasłoText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nie jest to na twojej liście."
        app:layout_constraintBottom_toTopOf="@+id/P4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.455"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/P3"
        app:layout_constraintVertical_bias="0.529" />

    <CheckedTextView
        android:id="@+id/P2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mleko"
        app:layout_constraintBottom_toTopOf="@+id/P3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.781" />

</androidx.constraintlayout.widget.ConstraintLayout>